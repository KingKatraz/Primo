<h1>Insert the form to create a new instance of your model.</h1>

<form method="POST" action="/skate_spots">
    <label for="name">Name:</label>
    <input type="text" id="name" name="name" /><br>
    
    <label for="location">Location:</label>
    <input type="text" id="location" name="location" /><br>
    
    <label for="heat_index">Security Level:</label>
   
    <select id="heat_index" name="heat_index">
       <option value="Chill">Chill</option>
        <option value="Moderate">Moderate</option>
        <option value="Hot">Hot</option>
        <option value="Red Hot">Red Hot</option>
    </select><br>
 
    <label for="skill_level">Skill Level:</label>
    <select id="skill_level" name="skill_level">
        <option value="Beginner">Beginnner</option>
        <option value="Amatuer">Amatuer</option>
        <option value="Professional">Professional</option>
        <option value="Any Skill Level">Any Skill Level</option>
    </select><br>

     <label for="description">Why is this spot cool?:</label>
    <input type="text" id="description" name="description" /><br>
    
    <input type="submit" value="submit"/>

</form>